<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.ajfqolee.spring.test.jsp.repository.RealtorRepository">
	
	<insert id="insertRealtor" parameterType="map">
		INSERT INTO
			`realtor`
		(
			`office`
			,`phoneNumber`
			,`address`
			,`grade`
		)
		VALUE
		(
			#{office}
			,#{phoneNumber}
			,#{address}
			,#{grade}
		)
	
	</insert>
	
	<select id="selectLastRealtor"
	resultType="com.ajfqolee.spring.test.jsp.domain.Realtor">
	
		SELECT 
			`id`
			,`office`
			,`phoneNumber`
			,`address`
			,`grade`
		FROM
			`realtor`
		ORDER BY 
			`id` DESC
		LIMIT
			1
	</select>
	

</mapper>




